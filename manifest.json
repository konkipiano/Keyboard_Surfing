{
    "name": "Keyboard Surfing",
    "version": "1.0.0",
    "manifest_version": 2,
    "description": "Surf the Internet without mouse",
    "icons": {
        "16": "icons/icon.png",
        "48": "icons/icon.png",
        "128": "icons/icon.png"
    },
    "content_scripts": [
        {
            "js": [
                "/js/lib/jquery.min.js",
                "/js/main.js"
            ],
            "css": [
                "/css/main.css"
            ]
        }
    ],
    "browser_action": {
        "default_icon": {
            "16": "icons/icon.png",
            "24": "icons/icon.png",
            "32": "icons/icon.png"
        },
        "default_title": "Keyboard Surfing"
    },
    "background": {
        "scripts": []
    }
}